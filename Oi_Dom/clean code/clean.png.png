    /* trackingUtm function */ 
    (() => { 
            let searchParams = new URL(window.location.href).searchParams; 
            if (!Array.from(searchParams.keys()).find(key => key.match(/^(utm_|gclid|fbclid)/))) { 
              return; 
            } 
            const newTrackingData = { 
              medium: searchParams.has('utm_medium') ? decodeURI(searchParams.get('utm_medium')) : '', 
              source: searchParams.has('utm_source') ? decodeURI(searchParams.get('utm_source')) : '', 
              name: searchParams.has('utm_campaign') ? decodeURI(searchParams.get('utm_campaign')) : '', 
              term: searchParams.has('utm_term') ? decodeURI(searchParams.get('utm_term')) : '', 
              content: searchParams.has('utm_content') ? decodeURI(searchParams.get('utm_content')) : '', 
              gclid: searchParams.has('gclid') ? decodeURI(searchParams.get('gclid')) : '', 
              fbclid: searchParams.has('fbclid') ? decodeURI(searchParams.get('fbclid')) : '', 
              date: new Date(), 
            }; 
            let trackingUtm;
            try { 
              trackingUtm = JSON.parse(localStorage.getItem('tracking_utm')); 
            } catch (e) { 
              trackingUtm = []; 
            }; 
            if (!trackingUtm) { 
              trackingUtm = [newTrackingData]; 
            } else { 
              if (Array.isArray(trackingUtm)) { 
                trackingUtm.push(newTrackingData); 
              } else { 
                trackingUtm = [trackingUtm, newTrackingData]; 
              } 
            } 
            window.localStorage['tracking_utm'] = JSON.stringify(trackingUtm); 
    })(); 
</script> 
